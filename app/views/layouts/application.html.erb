<!DOCTYPE html>
<html>
<%= render 'application/favicon' %>
  <%= render partial: 'shared/head' %>

  <%= body_tag do %>
    <%= render partial: 'shared/nav' %>
    <div class="notice"><%= notice %></div>
    <div class="alert"><%= alert %></div>
    <div class="container">
      <%= yield %>
    </div>
    <%= render partial: 'shared/footer' %>
  <% end %>
</html>
